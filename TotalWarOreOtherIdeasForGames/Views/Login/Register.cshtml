@model TotalWarDLA.Models.User;
@{

}

<h1>register</h1>
<h2 style="color:red;">@ViewBag.Error</h2>
<h4>User</h4>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Register">
            <div class="form-group">
                <label asp-for="UserName" class="control-label"></label>
                <input asp-for="UserName" class="form-group" />
                <span asp-validation-for="UserName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-group" />
                <span asp-validation-for="Email"></span>
            </div>
            <div class="form-group">
                <label asp-for="UiPassword" class="control-label"></label>
                <input asp-for="UiPassword" class="form-group" type="password" />
                <span asp-validation-for="UiPassword"></span>
            </div>
            <div class="form-grout">
                <input type="submit"class="btn btn-primary" value="Register" />
            </div>
        </form>
    </div>
</div>

<div class="row">
    <a asp-action="Index" class="btn btn-info">Go Back To Register</a>
</div>

